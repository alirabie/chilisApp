<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="iperfect.emasoft.com.chilisapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activites.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activites.Tablenumbers"
            android:label="@string/title_activity_tablenumbers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activites.Categories"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activites.Items" />
        <activity android:name=".Activites.Orders"></activity>
    </application>

</manifest>
